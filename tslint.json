{
    "extends": [
        "tslint:latest",
        "tslint-react"
    ],
    "rules": {
        "jsx-no-multiline-js": false,
        "jsx-no-lambda": false,
        "jsx-boolean-value": false,
        "jsx-wrap-multiline": false,
        "variable-name": false,
        "class-name": true,
        "comment-format": [
            true,
            "check-space"
        ],
        "member-ordering": [
            false
        ],
        "curly": true,
        "eofline": true,
        "forin": true,
        "indent": [
            true,
            "spaces"
        ],
        "label-position": true,
        "max-line-length": [
            true,
            250
        ],
        "no-arg": true,
        "object-literal-sort-keys": false,
        "no-bitwise": true,
        "no-console": [
            true,
            "log",
            "debug",
            "info",
            "time",
            "timeEnd",
            "trace"
        ],
        "no-construct": true,
        "no-debugger": true,
        "no-duplicate-variable": true,
        "no-empty": false,
        "no-eval": true,
        "no-shadowed-variable": true,
        "no-string-literal": true,
        "no-switch-case-fall-through": true,
        "arrow-parens": true,
        "ordered-imports": [
            true
        ],
        "trailing-comma": [
            true,
            {
                "multiline": "always",
                "singleline": "never"
            }
        ],
        "member-access": [
            false
        ],
        "no-trailing-whitespace": false,
        "no-unused-expression": true,
        "no-unused-variable": [
            true,
            "react"
        ],
        "no-use-before-declare": false,
        "no-var-keyword": true,
        "one-line": [
            true,
            "check-open-brace",
            "check-catch",
            "check-else",
            "check-whitespace"
        ],
        "radix": false,
        "semicolon": [
            true
        ],
        "triple-equals": [
            true,
            "allow-null-check"
        ],
        "whitespace": [
            true,
            "check-branch",
            "check-decl",
            "check-operator",
            "check-separator",
            "check-type"
        ],
        "quotemark": [
            true,
            "single",
            "jsx-double"
        ],
        "only-arrow-functions": [
            true,
            "allow-named-functions",
            "allow-declarations"
        ],
        "interface-name": [
            true
        ],
        "align": [
            true
        ],
        "object-literal-shorthand": true,
        "object-literal-key-quotes": [
            "consistent"
        ],
        "no-consecutive-blank-lines": [
            true
        ],
        "typedef-whitespace": [
            true
        ],
        "typedef": [
            true
        ],
        "use-isnan": true,
        "new-parens": true,
        "no-require-imports": true
    }
}